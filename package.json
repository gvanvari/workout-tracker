{
  "name": "workout-tracker",
  "version": "1.0.0",
  "description": "A lightweight, local-first workout tracking application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm -C backend run dev\" \"npm -C frontend run dev\"",
    "build": "npm -C backend run build && npm -C frontend run build",
    "start": "npm -C backend start"
  },
  "keywords": [
    "workout",
    "tracker",
    "fitness",
    "exercise",
    "logging"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
    "@types/axios": "^0.9.36",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "vite": "^7.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "package-lock": false
}
